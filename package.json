{
  "dependencies": {
    "@create-figma-plugin/utilities": "^1.8.2"
  },
  "devDependencies": {
    "@create-figma-plugin/build": "^1.8.2",
    "@create-figma-plugin/tsconfig": "^1.8.2",
    "@figma/plugin-typings": "1.40.0",
    "typescript": "^4"
  },
  "scripts": {
    "build": "build-figma-plugin --typecheck --minify",
    "watch": "build-figma-plugin --typecheck --watch"
  },
  "figma-plugin": {
    "editorType": [
      "figma"
    ],
    "id": "1069750945133910671",
    "name": "Zero Frame",
    "menu": [
      {
        "name": "Create Zero Height Frame",
        "main": {
          "src": "src/create-zero-frame.ts",
          "handler": "createZeroHeightFrame"
        },
        "parameters": [
          {
            "key": "length",
            "name": "Enter Frame width",
            "description": "Enter the width for your Zero Frame (defaults to 100px)",
            "allowFreeform": true,
            "optional": true
          }
        ]
      },
      {
        "name": "Create Zero Width Frame",
        "main": {
          "src": "src/create-zero-frame.ts",
          "handler": "createZeroWidthFrame"
        },
        "parameters": [
          {
            "key": "length",
            "name": "Enter Frame height",
            "description": "Enter the height for your Zero Frame (defaults to 100px)",
            "allowFreeform": true,
            "optional": true
          }
        ]
      },
      {
        "name": "Wrap in Zero Height Frame",
        "main": {
          "src": "src/wrap-in-zero-frame.ts",
          "handler": "wrapInZeroHeightFrame"
        }
      },
      {
        "name": "Wrap in Zero Width Frame",
        "main": {
          "src": "src/wrap-in-zero-frame.ts",
          "handler": "wrapInZeroWidthFrame"
        }
      },
      {
        "name": "Add Zero Height Frame to Auto Layout",
        "main": {
          "src": "src/add-to-autolayout.ts",
          "handler": "addZeroHeightFrameToAutoLayout"
        }
      },
      {
        "name": "Add Zero Width Frame to Auto Layout",
        "main": {
          "src": "src/add-to-autolayout.ts",
          "handler": "addZeroWidthFrameToAutoLayout"
        }
      }
    ]
  }
}
